<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_company_form_inherit_lanta" model="ir.ui.view">
            <field name="name">view_company_form_inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='website']" position="after">
                    <field name="image_production_1920" widget="image" class="oe_avatar"/>
                    <field name="image_production_2_1920" widget="image" class="oe_avatar"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>